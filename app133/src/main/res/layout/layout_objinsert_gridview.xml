<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:background="@color/background"
	android:orientation="horizontal" >
	<GridLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:columnCount="5"
		android:orientation="horizontal"
		>
		<ImageView
			android:id="@+id/objinsert_text"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/text"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			android:layout_margin="0dp"
			/>

		<ImageView
			android:id="@+id/objinsert_time"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/clock"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_counter"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/cnt"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_barcode"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/barcode"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_barcode_var"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/barcode_dyn"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_julian"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/julian"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_hour"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/letter_hour"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_graphic"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/image"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_week_year"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/woy"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>

		<ImageView
			android:id="@+id/objinsert_week_day"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/weekday"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>
		<!-- H.M.Wang 2020-1-21 追加Shift控件 -->
		<ImageView
			android:id="@+id/objinsert_shift"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/shift"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>
		<!-- End of H.M.Wang 2020-1-21 追加Shift控件 -->

		<!-- H.M.Wang 2020-2-16 追加HyperText控件 -->
		<ImageView
			android:id="@+id/objinsert_hypertext"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/hypertext"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>
		<!-- End of H.M.Wang 2020-2-16 追加HyperText控件 -->

		<!-- H.M.Wang 2020-6-10 追加DynamicText控件 -->
		<ImageView
			android:id="@+id/objinsert_dynamictext"
			android:layout_width="130dp"
			android:layout_height="130dp"
			android:layout_margin="5dp"
			android:background="@drawable/bg_mid_button"
			android:src="@drawable/dynamictext"
			android:scaleType="fitCenter"
			android:layout_gravity="center"
			/>
		<!-- End of 2020-6-10 追加DynamicText控件 -->

		<!--
            <Button
                    android:id="@+id/objinsert_line"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_margin="5dp"
                    android:background="@drawable/style_line"
                    android:button="@null"
                    android:layout_gravity="center"
                    />
            <Button
                    android:id="@+id/objinsert_rect"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_margin="5dp"
                    android:background="@drawable/style_rect"
                    android:button="@null"
                    android:layout_gravity="center"
                    />
            <Button
                    android:id="@+id/objinsert_ellipse"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_margin="5dp"
                    android:background="@drawable/style_ellipse"
                    android:button="@null"
                    android:layout_gravity="center"
                    />
             -->

	</GridLayout>
	<ImageView
		android:id="@+id/cancel_btn"
		android:layout_width="50dp"
		android:layout_height="50dp"
		android:src="@drawable/cancel_icon"
		android:scaleType="fitCenter"
		/>
</LinearLayout>